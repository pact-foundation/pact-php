            },
            "$.movies.movie.also-known-as.aka:united-states.#text": {
              "size": 10,
              "type": "RandomString"
            },
            "$.movies.movie.release-date.#text": {
              "format": "dd-MM-yyyy",
              "type": "Date"
            },
            "$.movies.movie.release-date.china.#text": {
              "format": "dd-MM-yyyy",
              "type": "Date"
            },
            "$.movies.movie.specs['@aspect-ratio']": {
              "regex": "([0-9]+[.])?[0-9]+:[0-9]+",
              "type": "Regex"
            },
            "$.movies.movie.specs['@color']": {
              "regex": "color|black&white",
              "type": "Regex"
            },
            "$.movies.movie.specs['@runtime']": {
              "regex": "\\d+ hours [0-5]?[0-9] minutes",
              "type": "Regex"
            }
          }
        },
        "headers": {
          "Content-Type": "application/xml"
        },
        "matchingRules": {
          "body": {
            "$.movies.movie": {
              "combine": "AND",
              "matchers": [
