{
  "consumer": {
    "name": "someConsumer"
  },
  "provider": {
    "name": "someProvider"
  },
  "interactions": [
    {
      "description": "A get request to /goodbye/{name}",
      "providerState": "Get Goodbye",
      "request": {
        "method": "GET",
        "path": "/goodbye/Bob",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "Goodbye, Bob"
        }
      },
      "metadata": null
    },
    {
      "description": "A get request to /hello/{name}",
      "request": {
        "method": "GET",
        "path": "/hello/Bob",
        "headers": {
          "Content-Type": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "message": "Hello, Bob"
        },
        "matchingRules": {
          "$.body.message": {
            "match": "regex",
            "regex": "(Hello, )[A-Za-z]+"
          }
        }
      },
      "metadata": null
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}